global:
  name: "consul"
  enabled: true
  recursors: ["{{ network_dns[0] }}", "{{ network_dns[1] }}"]
ui:
  enabled: true
  service:
    enabled: true
    type: "NodePort"
    nodePort:
      http: "{{ consul_http_port }}"
server:
  enabled: true
  replicas: "{{ consul_replicas }}"

client:
  enabled: true

dns:
  enabled: true

syncCatalog:
  enabled: true
  default: true
  toConsul: true
  toK8S: true
  consulPrefix: "k8s-"


